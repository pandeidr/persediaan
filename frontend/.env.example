# React App Configuration
REACT_APP_API_BASE_URL=http://localhost:5000/api
REACT_APP_APP_NAME=Persediaan Accounting
REACT_APP_VERSION=1.0.0

# Feature Flags
REACT_APP_ENABLE_DARK_MODE=true
REACT_APP_ENABLE_OFFLINE_MODE=false
REACT_APP_ENABLE_ANALYTICS=false

# Chart and Reporting Configuration
REACT_APP_DEFAULT_CHART_THEME=material
REACT_APP_MAX_CHART_DATA_POINTS=100

# File Upload Configuration
REACT_APP_MAX_FILE_SIZE=10485760
REACT_APP_ALLOWED_FILE_TYPES=.pdf,.jpg,.jpeg,.png,.doc,.docx,.xls,.xlsx

# Date/Time Configuration
REACT_APP_DEFAULT_DATE_FORMAT=MM/dd/yyyy
REACT_APP_DEFAULT_TIME_FORMAT=HH:mm:ss
REACT_APP_DEFAULT_TIMEZONE=America/New_York

# Pagination Configuration
REACT_APP_DEFAULT_PAGE_SIZE=25
REACT_APP_MAX_PAGE_SIZE=100

# Development Configuration
GENERATE_SOURCEMAP=false
FAST_REFRESH=true